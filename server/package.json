{
	"name": "my-app",
	"version": "1.0.0",
	"description": "Production-ready Express TypeScript application",
	"main": "dist/server.js",
	"type": "commonjs",
	"scripts": {
		"dev": "tsx watch src/server.ts",
		"build": "prisma generate && tsc && tsc-alias",
		"start": "node --enable-source-maps dist/server.js",
		"lint": "eslint src --ext .ts --fix",
		"format": "prettier --write \"src/**/*.ts\"",
		"clean": "rimraf dist",
		"prisma:generate": "prisma generate",
		"db:migrate": "prisma migrate deploy",
		"db:migrate:dev": "prisma migrate dev",
		"db:studio": "prisma studio"
	},
	"keywords": [
		"express",
		"typescript",
		"api"
	],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@prisma/adapter-pg": "^7.2.0",
		"@prisma/client": "^7.2.0",
		"@types/compression": "^1.8.1",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.5",
		"@types/morgan": "^1.9.10",
		"@types/pg": "^8.16.0",
		"compression": "^1.8.1",
		"cors": "^2.8.5",
		"dotenv": "^17.2.3",
		"express": "^5.1.0",
		"express-rate-limit": "^8.2.1",
		"groq-sdk": "^0.37.0",
		"helmet": "^8.1.0",
		"morgan": "^1.10.1",
		"pg": "^8.16.3",
		"pino": "^10.1.0",
		"pino-pretty": "^13.1.2",
		"prisma": "^7.2.0",
		"zod": "^4.2.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.1",
		"@types/node": "^24.10.0",
		"@typescript-eslint/eslint-plugin": "^8.46.3",
		"@typescript-eslint/parser": "^8.46.3",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.4",
		"prettier": "^3.6.2",
		"tsc-alias": "^1.8.16",
		"tsconfig-paths": "^4.2.0",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.3"
	}
}
